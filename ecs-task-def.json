{
  "family": "my-ecs-taskdef",
  "executionRoleArn": "arn:aws:iam::216056336046:role/ecsExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "my-ecs-app",
      "image": "216056336046.dkr.ecr.us-east-1.amazonaws.com/dev/test:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": [ "FARGATE" ],
  "cpu": "256",
  "memory": "512"
}
